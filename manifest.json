{
    "name": "Color-Picker",
    "description": "Color-Picker-Chrome-Extension-->(Pick any Color you Like)",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/color-picker-16.png",
        "32": "/images/color-picker-32.png",
        "48": "/images/color-picker-48.png",
        "96": "/images/color-picker-96.png",
        "128": "/images/color-picker-128.png"

    }
}